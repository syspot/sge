<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
	"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">

<hibernate-configuration>

	<session-factory>

		<property name="current_session_context_class">thread</property>

		<property name="hibernate.dialect">org.hibernate.dialect.PostgreSQLDialect</property>
		<property name="hibernate.connection.url">jdbc:postgresql://localhost:5432/sge</property>
		<property name="hibernate.connection.driver_class">org.postgresql.Driver</property>
		<property name="hibernate.connection.username">postgres</property>
		<property name="hibernate.connection.password">15rt01</property>

		<property name="hibernate.show_sql">false</property>
		<property name="hibernate.format_sql">true</property>

		<mapping class="br.com.sge.model.Menu" />
		<mapping class="br.com.sge.model.Grupo" />
		<mapping class="br.com.sge.model.Usuario" />
		<mapping class="br.com.sge.model.Permissao" />
		<mapping class="br.com.sge.model.Fonte" />
		<mapping class="br.com.sge.model.Operador" />
		<mapping class="br.com.sge.model.TipoEquipamento" />
		<mapping class="br.com.sge.model.TipoIdentificador" />
		<mapping class="br.com.sge.model.TipoServico" />
		<mapping class="br.com.sge.model.Cliente" />
		<mapping class="br.com.sge.model.Contrato" />
		<mapping class="br.com.sge.model.Fornecedor" />
		<mapping class="br.com.sge.model.Equipamento" />
		<mapping class="br.com.sge.model.Agenda" />
		<mapping class="br.com.sge.model.Medicao" />
		<mapping class="br.com.sge.model.Financeiro" />
		<mapping class="br.com.sge.model.FinanceiroParcial" />
		<mapping class="br.com.sge.model.TipoTransacao" />
		

	</session-factory>

</hibernate-configuration>
	